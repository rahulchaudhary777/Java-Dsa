class linear{
    public static void main(String []args){
    
        int []arr = {1,3,8,4,9,2,0};
        int data = 9;

        for(int i=0;i<arr.length;i++){
            if(arr[i] == data){
                System.out.println("element found at index: "+i);
                break;
            }
        }
        if(i == arr.length)
            System.out.println("element not found");
    }
}
